{
  "version": 2,
  "builds": [
    {
      "src": "slack-app.js",
      "use": "@vercel/node"
    },
    {
      "src": "webhook-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/slack-app",
      "dest": "slack-app.js"
    },
    {
      "src": "/webhook-server",
      "dest": "webhook-server.js"
    },
    {
      "src": "/",
      "dest": "webhook-server.js"
    }
  ]
}
